{
  "name": "fuhttp-ts",
  "version": "0.1.31",
  "description": "a HTTP-server written in TypeScript using Node.js",
  "main": "build/fuhttp.js",
  "typings": "build/fuhttp.d.ts",
  "scripts": {
    "prepublish": "typings install && npm run rebuild",
    "typedoc": "cd ./src/ && typedoc ./ --out ../documentation/ --tsconfig ./tsconfig.json --media ../media --target es5",
    "rebuild": "rm -rf ./build/ && grunt build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fosenutvikling/fuHTTP-ts.git"
  },
  "keywords": [
    "ts",
    "fu",
    "http"
  ],
  "author": "Jonas Kirkemyr <jonas@fosen-utvikling.no> (http://fosen-utvikling.no)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fosenutvikling/fuHTTP-ts/issues"
  },
  "homepage": "https://github.com/fosenutvikling/fuHTTP-ts#readme",
  "dependencies": {
    "crossroads": "^0.12.2",
    "formidable": "^1.0.17"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-ts": "^5.5.1"
  }
}